<section>
    <div class="container">
      <div class="row">
        <a href="/admin/add-products" class="btn btn-success ml-auto mt-5">Add products</a>

      </div>
        <table class="table table-striped table-dark mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">title</th>
      <th scope="col">category</th>
      <th scope="col">discription</th>
      <th>image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.description}}</td>
      <td><img style="width:50px;height:50px" src={{this.image}}</td>
    </tr>
    {{/each}}
     
  </tbody>
</table>
    </div>
</section>